    "publish": "npm run publish"
  },
  "devDependencies": {
    "bun-plugin-dts": "^0.2.1",
    "bun-types": "^1.0.6",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "prettier-config-whatskit": "^0.4.0",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "env": {
      "browser": false,
      "es6": true,
      "node": true
    },
    "extends": [
      "plugin:prettier/recommended"
    ]
  },
  "eslintIgnore": [
    "*.mjs",
    "lib/*",
    "dist/*",
    "build/*",
    "coverage/*",
    "fixtures/*",
    "umd/*"
