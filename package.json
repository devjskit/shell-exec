{
  "name": "@whatskit/shell-exec",
  "version": "0.1.0",
  "license": "MIT",
  "description": "A tiny cross-platform promise based wrapper around child_process.spawn.",
  "author": "Billgo <hi@billgo.me>",
  "bugs": "https://github.com/whatskit/shell-exec/issues",
  "repository": "git@github.com:whatskit/shell-exec",
  "prettier": "prettier-config-whatskit",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "bun"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "bun run bun.build.mjs",
